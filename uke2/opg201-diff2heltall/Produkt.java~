import java.util.Scanner;
import java.lang.Math;
/** 
    This program asks the user for input in the terminal
    and computes the difference
*/

class Differanse {
    public static void main(String[] args) {
	int x;
	int y;
	int diff;
	Scanner in;
	System.out.print("Oppgi verdien til x: ");
	in = new Scanner(System.in);
	x = in.nextInt();
        
	System.out.print("Oppgi verdien til y: ");
	in = new Scanner(System.in);
	y = in.nextInt();
	diff = x - y;
	System.out.print("Differansen mellom x og y er ");
	System.out.print(x - y);
	System.out.println(".");
	System.out.print("Absoluttverdien er: ");
	System.out.println(Math.abs(diff));
    }
}
